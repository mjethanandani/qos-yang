<!--
    This example shows a QoS policy configuration.
-->

<?xml version="1.0" encoding="UTF-8"?>
<policies
  xmlns="urn:ietf:params:xml:ns:yang:ietf-qos-policy"
  xmlns:classifier="urn:ietf:params:xml:ns:yang:ietf-qos-classifier">
  <policy>
    <name>my-policy</name>
    <!-- 
	 Add the type of policy here.
    -->
    <classifier>
      <name>my-classifier</name>
      <inline>true</inline>
      <filter-operation>match-any-filter</filter-operation>
      <filter>
	<!-- 
	     Add the type of filter here.
	-->
	<logical-not>true</logical-not>
      </filter>
      <action>
	<!-- 
	     Add the type of action and action-params here.
	-->
      </action>
    </classifier>
  </policy>
</policies>

